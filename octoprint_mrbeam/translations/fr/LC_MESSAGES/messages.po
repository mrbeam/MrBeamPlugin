#: octoprint_mrbeam/templates/laser_job_done.jinja2:30
msgid "Laser job duration:"
msgstr "Durée du travail :"

#: octoprint_mrbeam/templates/laser_job_done.jinja2:31
msgid "Estimated:"
msgstr "Estimation :"

